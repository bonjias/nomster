<% @places.each do |place | %>
	<div class="booyah-box col-xs-10 col-xs-offset-1"> 
		<h1> <%= place.name %> </h1>
		<i>  <%= place.adress %> </i><br /><br /> 
		<p>  <%= place.discription %> </p><br /> 
	</div> 
<% end %> 



<%= will_paginate @places, previous_label: "Older", next_label: "Newer" %> 